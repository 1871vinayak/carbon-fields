{
  "name": "carbon-fields",
  "version": "1.5.0",
  "description": "WordPress developer-friendly custom fields for post types, taxonomy terms, users, comments, widgets, options, navigation menus and more.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest --config .jest",
    "css": "postcss ./assets/css/main.css -o ./assets/dist/carbon.css",
    "css:dev": "npm run css -- -w",
    "webpack:vendor": "webpack --config webpack.vendor.js --hide-modules",
    "webpack:core": "npm run webpack:vendor && webpack --config webpack.core.js --watch --hide-modules",
    "webpack:boot": "webpack --config webpack.boot.js --watch --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/htmlburger/carbon-fields.git"
  },
  "keywords": [
    "custom",
    "fields"
  ],
  "author": "htmlburger",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/htmlburger/carbon-fields/issues"
  },
  "homepage": "https://github.com/htmlburger/carbon-fields#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cssnano": "^3.10.0",
    "enzyme": "^2.8.0",
    "expose-loader": "^0.7.3",
    "jest": "^19.0.2",
    "jquery": "1.12",
    "postcss-cli": "^3.1.1",
    "postcss-import": "^9.1.0",
    "postcss-simple-vars": "^3.1.0",
    "react-addons-test-utils": "^15.5.1",
    "redux-mock-store": "^1.2.3",
    "redux-saga-test-plan": "^2.3.6",
    "webpack": "^2.3.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "fecha": "^2.3.0",
    "locutus": "^2.0.8",
    "lodash": "^4.17.4",
    "object-path-immutable": "^0.5.1",
    "prop-types": "^15.5.6",
    "react": "^15.5.3",
    "react-color": "^2.11.4",
    "react-dom": "^15.5.3",
    "react-onclickoutside": "^5.11.1",
    "react-redux": "^5.0.4",
    "recompose": "^0.23.1",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-saga": "^0.14.6",
    "reselect": "^3.0.0"
  }
}
