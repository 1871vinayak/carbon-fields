{
  "name": "carbon-fields",
  "version": "1.5.0.0",
  "description": "WordPress developer-friendly custom fields for post types, taxonomy terms, users, comments, widgets, options, navigation menus and more.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "bin": {
    "postcss": "node_modules/.bin/postcss"
  },
  "scripts": {
    "test": "jest --config .jest",
    "postcss": "postcss -c postcss.config.js -w",
    "webpack:vendor": "webpack --config webpack.vendor.js --hide-modules",
    "webpack:app": "npm run webpack:vendor && webpack --config webpack.app.js --watch --hide-modules",
    "webpack:boot": "webpack --config webpack.boot.js --watch --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/htmlburger/carbon-fields.git"
  },
  "keywords": [
    "custom",
    "fields"
  ],
  "author": "htmlburger",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/htmlburger/carbon-fields/issues"
  },
  "homepage": "https://github.com/htmlburger/carbon-fields#readme",
  "devDependencies": {
    "autoprefixer": "^6.5.2",
    "babel-core": "^6.22.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "cssnano": "^3.8.0",
    "enzyme": "^2.7.0",
    "jest": "^18.1.0",
    "jquery": "1.12",
    "postcss-cli": "^2.6.0",
    "postcss-import": "^8.1.3",
    "postcss-simple-vars": "^3.0.0",
    "react-addons-test-utils": "^15.4.2",
    "redux-mock-store": "^1.2.1",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.22.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "object-path-immutable": "^0.5.1",
    "react": "^15.4.2",
    "react-color": "^2.11.1",
    "react-dom": "^15.4.2",
    "react-onclickoutside": "^5.8.4",
    "react-redux": "^5.0.2",
    "recompose": "^0.22.0",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1",
    "redux-saga": "^0.14.3",
    "reselect": "^2.5.4"
  }
}
